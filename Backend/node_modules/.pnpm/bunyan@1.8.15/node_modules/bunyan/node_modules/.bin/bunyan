#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Pr/Desktop/JWT/Backend/node_modules/.pnpm/bunyan@1.8.15/node_modules/bunyan/bin/node_modules:/mnt/c/Users/Pr/Desktop/JWT/Backend/node_modules/.pnpm/bunyan@1.8.15/node_modules/bunyan/node_modules:/mnt/c/Users/Pr/Desktop/JWT/Backend/node_modules/.pnpm/bunyan@1.8.15/node_modules:/mnt/c/Users/Pr/Desktop/JWT/Backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Pr/Desktop/JWT/Backend/node_modules/.pnpm/bunyan@1.8.15/node_modules/bunyan/bin/node_modules:/mnt/c/Users/Pr/Desktop/JWT/Backend/node_modules/.pnpm/bunyan@1.8.15/node_modules/bunyan/node_modules:/mnt/c/Users/Pr/Desktop/JWT/Backend/node_modules/.pnpm/bunyan@1.8.15/node_modules:/mnt/c/Users/Pr/Desktop/JWT/Backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/bunyan" "$@"
else
  exec node  "$basedir/../../bin/bunyan" "$@"
fi
